package com.robotoworks.mechanoid.example.flickr.content

database Flickr {
	
	actions {
		SearchInterestingess 'interestingness/search_suggest_query'
	}
	
	migration {
		create table interestingness (
			_id integer primary key autoincrement,
			photo_id text,
			owner text,
			secret text,
			server text,
			farm integer,
			title text,
			is_public integer,
			is_friend integer,
			is_family integer
		);
	}
}