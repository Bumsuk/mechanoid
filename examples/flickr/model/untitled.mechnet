package com.robotoworks.machanoid.example.flickr.net

type generate Photo {
	id:int,
	owner:int
}

client FlickrClient "http://api.flickr.com/" {
	 get getInterestingnessList "?method=flicker.interesting&format=json" {
	 	params api_key:String&nojsoncallback:int
	 	response {
	 		wrap-with "photos" {
	 			page:int
	 		}
	 	}
	 }
}